<Dora>
	<Node X="{ x or 0 }" Y="{ y or 0 }" Width="426" Height="92" AnchorX="0">
		<Sprite File="ai.clip|ai_big_tab_1" X="{ $X }" Y="{ $Y }"/>
		<Label Text="{ text }" FontName="SourceHanSansCN-Regular"
			FontSize="45" Color3="0xffffff" TextAlign="Center"
			X="{ $X - 20 }" Y="{ $Y - 5 }"/>
	</Node>
</Dora>
